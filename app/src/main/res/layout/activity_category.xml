<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.hanium.SJnJH.CategoryActivity">


    <ImageButton
        android:id="@+id/btBack"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_margin="10dp"
        android:layout_marginTop="10dp"
        android:backgroundTint="@color/transparent"
        android:src="@drawable/ic_baseline_arrow_back_24"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_marginTop="15dp"
        android:id="@+id/titleTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="Category"
        android:textColor="@color/mainColor"
        android:textSize="30sp"
        android:textStyle="italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/titleTv">


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:orientation="horizontal">

            <ImageButton
                android:scaleType="centerCrop"
                android:backgroundTint="@color/transparent"
                android:id="@+id/chickenBt"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:src="@drawable/chicken"
                tools:layout_editor_absoluteX="57dp"
                tools:layout_editor_absoluteY="114dp" />

            <ImageButton
                android:scaleType="centerCrop"
                android:backgroundTint="@color/transparent"
                android:id="@+id/pizzaBt"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:layout_marginLeft="60dp"
                android:layout_toRightOf="@+id/chickenBt"
                android:src="@drawable/pizza"

                tools:layout_editor_absoluteX="295dp"
                tools:layout_editor_absoluteY="177dp" />

        </LinearLayout>


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:orientation="horizontal">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/pizzaBt"
                android:layout_marginLeft="30dp"
                android:text="치킨"
                android:textColor="@color/black"
                android:textSize="30sp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="220dp"
                tools:layout_editor_absoluteY="131dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/pizzaBt"
                android:layout_marginLeft="130dp"
                android:text="피자"
                android:textColor="@color/black"
                android:textSize="30sp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="220dp"
                tools:layout_editor_absoluteY="131dp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:orientation="horizontal">

            <ImageButton
                android:scaleType="centerCrop"
                android:backgroundTint="@color/transparent"
                android:id="@+id/hamBt"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:src="@drawable/hamburger"
                tools:layout_editor_absoluteX="57dp"
                tools:layout_editor_absoluteY="114dp" />

            <ImageButton
                android:scaleType="centerCrop"
                android:backgroundTint="@color/transparent"
                android:id="@+id/chinaBt"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:layout_marginLeft="60dp"
                android:layout_toRightOf="@+id/chickenBt"
                android:src="@drawable/noodles"
                tools:layout_editor_absoluteX="295dp"
                tools:layout_editor_absoluteY="177dp" />

        </LinearLayout>


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:orientation="horizontal">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/pizzaBt"
                android:layout_marginLeft="20dp"
                android:text="햄버거"
                android:textColor="@color/black"
                android:textSize="30sp"
                android:textStyle="bold"

                tools:layout_editor_absoluteX="220dp"
                tools:layout_editor_absoluteY="131dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/pizzaBt"
                android:layout_marginLeft="115dp"
                android:text="중식"
                android:textColor="@color/black"
                android:textSize="30sp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="220dp"
                tools:layout_editor_absoluteY="131dp" />

        </LinearLayout>


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:orientation="horizontal">

            <ImageButton
                android:scaleType="centerCrop"
                android:backgroundTint="@color/transparent"
                android:id="@+id/koreaBt"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:src="@drawable/tteokbokki"
                tools:layout_editor_absoluteX="57dp"
                tools:layout_editor_absoluteY="114dp" />

            <ImageButton
                android:scaleType="centerCrop"
                android:backgroundTint="@color/transparent"
                android:id="@+id/westBt"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:layout_marginLeft="60dp"
                android:layout_toRightOf="@+id/chickenBt"
                android:src="@drawable/spaguetti"
                tools:layout_editor_absoluteX="295dp"
                tools:layout_editor_absoluteY="177dp" />

        </LinearLayout>


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:orientation="horizontal">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/pizzaBt"
                android:layout_marginLeft="30dp"
                android:text="분식"
                android:textColor="@color/black"
                android:textSize="30sp"
                android:textStyle="bold"

                tools:layout_editor_absoluteX="220dp"
                tools:layout_editor_absoluteY="131dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/pizzaBt"
                android:layout_marginLeft="130dp"
                android:text="양식"
                android:textColor="@color/black"
                android:textSize="30sp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="220dp"
                tools:layout_editor_absoluteY="131dp" />

        </LinearLayout>

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>